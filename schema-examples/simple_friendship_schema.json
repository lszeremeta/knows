{
    "$schema": "https://raw.githubusercontent.com/lszeremeta/knows/main/schema.json",
    "nodeLabel": "Person",
    "edgeLabel": "friendOf",
    "nodeProperties": {
        "name": "Name",
        "age": {"type": "Int", "min": 18, "max": 80}
    },
    "edgeProperties": {
        "since": {"type": "Date", "symmetric": true}
    },
    "computedNodeProperties": {
        "friendCount": "degree"
    }
}
